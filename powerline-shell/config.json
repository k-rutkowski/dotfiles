{
  "segments": [
    "virtual_env",
    "ssh",
    "cwd",
	"svn",
    "git",
    "git_stash",
    "jobs",
	"set_term_title",
    "exit_code",
	"newline",
    "root"
  ],
  "_mode": "patched",
  "cwd": {
	  "_mode": "plain",
	  "max_depth": 6,
	  "max_dir_size": 12,
	  "full_cwd": true
  },
  "vcs": {
	  "_show_symbol": true
  },
  "_theme": "gruvbox"
}
